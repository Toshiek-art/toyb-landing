{
  "name": "toyb",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --check",
    "format:write": "prettier . --write",
    "typecheck": "tsc --noEmit",
    "check": "npm run typecheck && astro check",
    "test:email": "node scripts/test-email.mjs",
    "test:a11y": "node scripts/test-a11y.mjs",
    "deploy": "npm run build && wrangler pages deploy dist --project-name \"${CLOUDFLARE_PAGES_PROJECT:-toyb}\""
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.12",
    "@astrojs/sitemap": "^3.7.0",
    "@supabase/supabase-js": "^2.97.0",
    "astro": "^5.17.3",
    "nodemailer": "^8.0.1"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@axe-core/playwright": "^4.11.1",
    "@eslint/js": "^10.0.1",
    "@types/nodemailer": "^7.0.11",
    "eslint": "^10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.6.0",
    "globals": "^17.3.0",
    "playwright": "^1.58.2",
    "prettier": "^3.8.1",
    "prettier-plugin-astro": "^0.14.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "wrangler": "^4.67.0"
  }
